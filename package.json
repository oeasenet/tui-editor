{
    "name": "@oeasenet/tui-editor",
    "version": "0.0.2",
    "description": "Vue 3 wrapper components for Toast UI Editor",
    "author": "oeasenet",
    "license": "MIT",
    "keywords": [
        "vue",
        "vue3",
        "toast-ui",
        "editor",
        "markdown",
        "wysiwyg"
    ],
    "scripts": {
        "build": "rimraf dist && vue-tsc --noEmit && vite build && vue-tsc --project tsconfig.build.json",
        "lint": "eslint --ext .ts,.vue src",
        "docs:dev": "vitepress dev docs",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs"
    },
    "peerDependencies": {
        "vue": "^3.3.4"
    },
    "dependencies": {
        "@toast-ui/editor": "^3.2.2",
        "@toast-ui/editor-plugin-chart": "^3.0.1",
        "@toast-ui/editor-plugin-code-syntax-highlight": "^3.1.0",
        "@toast-ui/editor-plugin-color-syntax": "^3.1.0",
        "@toast-ui/editor-plugin-table-merged-cell": "^3.1.0",
        "@toast-ui/editor-plugin-uml": "^3.0.1",
        "prismjs": "^1.30.0"
    },
    "devDependencies": {
        "@types/node": "^22.14.1",
        "@typescript-eslint/eslint-plugin": "^7.0.0",
        "@typescript-eslint/parser": "^7.0.0",
        "@vitejs/plugin-vue": "^5.2.3",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.0.0",
        "eslint-plugin-vue": "^9.33.0",
        "rimraf": "^6.0.1",
        "sass": "^1.86.3",
        "typescript": "^5.8.3",
        "vite": "^6.3.2",
        "vitepress": "^1.6.3",
        "vue-tsc": "^2.2.8"
    },
    "files": [
        "dist",
        "src/"
    ],
    "main": "./dist/tui-editor-vue3.umd.js",
    "module": "./dist/tui-editor-vue3.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/types/index.d.ts",
            "import": "./dist/tui-editor-vue3.es.js",
            "require": "./dist/tui-editor-vue3.umd.js"
        },
        "./dist/tui-editor.css": "./dist/tui-editor.css"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oeasenet/tui-editor#main"
    },
    "bugs": {
        "url": "https://github.com/oeasenet/tui-editor/issues"
    },
    "homepage": "https://github.com/oeasenet/tui-editor#readme"
}
